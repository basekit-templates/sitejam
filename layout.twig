<head>
	<meta charset="utf-8" />
	<title>{{ page.title }}</title>
	<meta name="keywords" content="{{ page.keywords }}" />
	<meta name="description" content="{{ page.description }}" />
	<meta http-equiv="content-language" content="{{ page.seoLang }}" />

	{% if site.favicon %}
		<link rel="icon" href="{{ site.favicon['favicon'] }}" type="image/x-icon" />
		<link rel="SHORTCUT ICON" href="{{ site.favicon['thumbnail'] }}?v={{ "now"|date("U") }}" type="image/x-icon" />
	{% endif %}
	<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
	<meta name="viewport" content="width=device-width,initial-scale=1">

	<link rel="stylesheet" type="text/css" href="{{asset('/fonts/fontawesome/css/font-awesome.min.css')}}" />
	<link rel="stylesheet" type="text/css" href="{{asset('/css/resNav.css')}}" />
	<link rel="stylesheet" type="text/css" href="{{asset('/css/magnific-popup.css')}}" />

	{% include basekit.headScript %}
	
</head>
<body class="{{ page.backgroundClass }}">

	{% block content %}
	{% endblock %}

	{% include basekit.bodyScript %}

	<script src="//ajax.googleapis.com/ajax/libs/jquery/1.10.2/jquery.min.js"></script>
	<script>window.jQuery || document.write('<script src="{{ asset('libs/jquery-1.10.2.min.js')}}"><\/script>')</script>

	<!-- RESPONSIVE NAV !-->
	<script src="{{asset('/js/responsiveNav/modernizr.custom.js')}}"></script>
	<script src="{{asset('/js/responsiveNav/jquery.dlmenu.js')}}"></script>
	<script src="{{asset('/js/jquery.magnific-popup.min.js')}}"></script> 
	<script>
	    $(function() {
	        $( '#dl-menu' ).dlmenu();
	    });
	</script>
	<script>
		$(document).ready(function() {
		$('.popup-youtube').magnificPopup({
			disableOn: 700,
			type: 'iframe',
			mainClass: 'mfp-fade',
			removalDelay: 160,
			preloader: false,

			fixedContentPos: false
		});
	});
	</script>

	<!-- HIDE / SHOW ONE !-->
	<script type="text/javascript">
	$(document).ready(function(){

	        $(".slidingDiv.one").hide();
	        $(".show_hide.one").show();
	 
	    $('.show_hide.one').click(function(){
	    $(".slidingDiv.one").slideToggle();
	    });
	 
	});
	</script>

	<!-- HIDE / SHOW TWO !-->
	<script type="text/javascript">
	$(document).ready(function(){
		
	        $(".slidingDiv.two").hide();
	        $(".show_hide.two").show();
	 
	    $('.show_hide.two').click(function(){
	    $(".slidingDiv.two").slideToggle();
	    });
	 
	});
	</script>

	<!-- HIDE / SHOW THREE !-->
	<script type="text/javascript">
	$(document).ready(function(){
		
	        $(".slidingDiv.three").hide();
	        $(".show_hide.three").show();
	 
	    $('.show_hide.three').click(function(){
	    $(".slidingDiv.three").slideToggle();
	    });
	 
	});
	</script>

	<script type="text/javascript">
	function toggleSlides(){
		$('.toggler').click(function(e){
			var id=$(this).attr('id');
			var widgetId=id.substring(id.indexOf('-')+1,id.length);
			$('#'+widgetId).slideToggle();
			$(this).toggleClass('sliderExpanded');
			$('.closeSlider').click(function(){
					$(this).parent().hide('slow');
					var relatedToggler='toggler-'+$(this).parent().attr('id');
					$('#'+relatedToggler).removeClass('sliderExpanded');
			});
		});
	};
	$(function(){
		toggleSlides();
	});
	</script>

	<script type="text/javascript">
    var prices = {
        "gbp": [0, 7, 12],
        "aud": [0, 25, 45],
        "usd": [0, 10, 19],
        "eur": [0, 9, 15]
    }

    $('.currency-switcher select').change(function () {
        switch(this.value) {
        case 'usd':
            $('.launch-price').text('$' + prices.usd[0]);
            $('.basic-price').text('$' + prices.usd[1]);
            $('.business-price').text('$' + prices.usd[2]);
            break;
        case 'gbp':
            $('.launch-price').text('£' + prices.gbp[0]);
            $('.basic-price').text('£' + prices.gbp[1]);
            $('.business-price').text('£' + prices.gbp[2]);
            break;
        case 'aud':
            $('.launch-price').text('AUD$' + prices.aud[0]);
            $('.basic-price').text('AUD$' + prices.aud[1]);
            $('.business-price').text('AUD$' + prices.aud[2]);
            break;
        case 'eur':
            $('.launch-price').text('€' + prices.eur[0]);
            $('.basic-price').text('€' + prices.eur[1]);
            $('.business-price').text('€' + prices.eur[2]);
            break;
		}
	});
	</script>

</body>
</html>




